{
  "name": "rrw-project-starter",
  "version": "1.0.0",
  "description": "\"custom react-redux-wepback starter\"",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "babel-tape-runner test/*.js -| tap-spec",
    "start": "nodemon server/app.js --ignore components",
    "css:build": "node-sass --include-path scss client/scss/main.scss public/resources/styles/style.css",
    "css:watch": "nodemon -e scss -x \" npm run css:build \""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sean-johnson/rrw-project-starter.git"
  },
  "author": "Sean Johnson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sean-johnson/rrw-project-starter/issues"
  },
  "homepage": "https://github.com/sean-johnson/rrw-project-starter#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "express": "^4.14.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-tape-runner": "^2.0.1",
    "colors": "^1.1.2",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
